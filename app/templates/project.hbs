{{#view "left-tem"}}
<div class="page-header">
	<h3>WHAT HAVE I DONE<small class="text-capitalize"> {{name}} </small> </h3>
</div>
<div class="project_list">
	<div>
		<form class="form-horizontal">
		  <div class="form-group">
		    <label class="col-sm-2 control-label">During</label>
		    <div class="col-sm-10">
		      <p class="form-control-static">{{formatted-date startTime}} - {{formatted-date endTime}}</p>
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="col-sm-2 control-label">My Role</label>
		    <div class="col-sm-10">
		      <p class="form-control-static">{{role}}</p>
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="col-sm-2 control-label">Description</label>
		    <div class="col-sm-10">
		      <p class="form-control-static">{{description}}</p>
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="col-sm-2 control-label">Responsibility</label>
		    <div class="col-sm-10">
		      <p class="form-control-static">{{responsibility}}</p>
		    </div>
		  </div>
		  <div class="form-group">
		    <label class="col-sm-2 control-label">Development Tools</label>
		    <div class="col-sm-10">
		      <p class="form-control-static">{{tools}}</p>
		    </div>
		  </div>
		</form></div>
</div>
{{/view}}
